with consulta_inside AS (
SELECT 
pp.proposal_id,
pp.proposal.insured_data.full_name,
pp.proposal.insured_data.email,
pp.status,
pp.proposal.sale_channel,
ae.macro_channel,
ae.channel_groupment,
pp.status_detail.reason,
pp.status_detail.description

FROM `azos-data.st_mongo_backoffice_underwriting.proposal` as pp
LEFT JOIN `azos-data.azos_harmonized.attribution_engine` as ae
USING (proposal_id)
WHERE pp.status = 'refused' AND pp._created >= '2025-09-11'
)

SELECT

*

FROM consulta_inside
WHERE channel_groupment = 'Inside Sales'
